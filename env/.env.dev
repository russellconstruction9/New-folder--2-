# Development Environment Variables
# Copy to .env.local and fill with your actual keys

# Google Maps API Key (from your existing code)
VITE_GOOGLE_MAPS_API_KEY=AIzaSyAyS8VmIL-AbFnpm_xmuKZ-XG8AmSA03AM

# Gemini AI API Key (you need to get this from Google AI Studio)
VITE_GEMINI_API_KEY=get_your_key_from_google_ai_studio

# Backend Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_BACKEND_URL=http://localhost:8000

# File Upload Settings
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Application Settings
VITE_ENVIRONMENT=development
VITE_DEBUG=true
VITE_ENABLE_AI=true
VITE_ENABLE_MAPS=true
VITE_ENABLE_OFFLINE=true

# Backend Database (when you set up your backend)
DATABASE_URL=postgresql://username:password@localhost:5432/constructtrack_dev
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=constructtrack_dev
DATABASE_USER=constructtrack_user
DATABASE_PASSWORD=your_secure_password

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# Redis (for caching)
REDIS_URL=redis://localhost:6379

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-specific-password

# AWS S3 Configuration (for file uploads)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_BUCKET_NAME=constructtrack-dev-uploads

# Logging
LOG_LEVEL=debug